<Window x:Class="ESILV.CloudApplication.Problem.JsonViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESILV.CloudApplication.Problem"
        mc:Ignorable="d"
        Title="JsonViewer" Height="300" Width="300">
    <Grid>
        <ListView x:Name="listViewJson">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Reason" Width="120" DisplayMemberBinding="{Binding _id}" />
                    <GridViewColumn Header="Count" Width="50" DisplayMemberBinding="{Binding value}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
