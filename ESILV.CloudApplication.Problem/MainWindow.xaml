<Window x:Class="ESILV.CloudApplication.Problem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESILV.CloudApplication.Problem"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>

        <!-- Buttons arranged vertically inside a StackPanel. -->
        <StackPanel HorizontalAlignment="Left">
            <Button Click="Button_Click">Compter le nombre de documents</Button>
            <Button Click="Button_Click_1">Charger un csv</Button>
            <Button Click="Button_Click_2">Button 3</Button>
        </StackPanel>
        <TextBox Name="MaxCities" HorizontalAlignment="Left" Height="24" Margin="296,37,0,0" TextWrapping="Wrap" Text="Max cities" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
